<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <title>Your Cars</title>
  </head>
  <body class="index-body">
    <%- include('../partials/_navbar.ejs') %>
    
    <div class="all-apps-body">
        <div class="all-apps-header">
    <h1>Your Garage</h1>
    <a href="/users/<%=user._id%>/cars/new">Add New Car</a>

</div>
<ul>
    <% cars.forEach((car)=>{ %>
    
      
      <a href="/users/<%= user._id %>/cars/<%= car._id %>">
        <li> <%= car.make %>  <%= car.model %>
      </a>
    </li>
    <% }) %>
  </ul>
  </div>
  </body>
</html>