<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <title>Your Cars</title>
  </head>
  <body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>Your Garage</h1>
    <a href="/users/<%=user._id%>/cars/new">Add New Car</a>


<ul>
    <% cars.forEach((car)=>{ %>
    <li>
      
      <a href="/users/<%= user._id %>/cars/<%= car._id %>">
        <%= car.make %>  <%= car.model %>
      </a>
    </li>
    <% }) %>
  </ul>
  
  </body>
</html>